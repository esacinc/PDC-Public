{
  "name": "kit",
  "version": "1.0.0",
  "description": "Starting point for the Apollo GraphQL Server tutorial.",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.0",
    "aws-sdk": "^2.499.0",
    "body-parser": "^1.18.2",
    "casual": "^1.5.19",
    "cors": "^2.8.4",
    "event-stream": "^4.0.1",
    "express": "^4.16.2",
    "google-auth-library": "^1.3.1",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.13.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.5",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.2",
    "node-fetch": "^1.7.3",
    "redis": "^2.8.0",
    "sequelize": "^5.15.1",
    "universal-analytics": "^0.4.20",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "co-mocha": "^1.2.2",
    "coveralls": "^3.0.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.18.10",
    "nyc": "^14.0.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "pdc/**/*.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "docs/reports/coverage"
  },
  "scripts": {
    "start": "nodemon ./server.js --exec babel-node",
    "test": "nyc mocha --timeout=90000 --compilers js:babel-register --require babel-polyfill --no-deprecation",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PDC/graphql.git"
  },
  "homepage": "https://github.com/PDC/graphql.git#readme"
}
