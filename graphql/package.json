{
  "name": "kit",
  "version": "1.0.0",
  "description": "Starting point for the Apollo GraphQL Server tutorial.",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "3.583.0",
    "@aws-sdk/client-s3": "3.705.0",
    "@aws-sdk/client-secrets-manager": "3.583.0",
    "@aws-sdk/cloudfront-signer": "3.696.0",
    "@aws-sdk/s3-request-presigner": "3.705.0",
    "apollo-server-express": "^3.13.0",
    "bunyan": "^1.8.15",
    "colorspace": "1.1.2",
    "cors": "^2.8.4",
    "express": "^4.21.0",
    "geoip-lite": "^1.4.2",
    "glob": "^7.1.6",
    "graphql": "^15.0.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.10.1",
    "redis": "^3.1.2",
    "sequelize": "7.0.0-alpha.9",
    "universal-analytics": "^0.5.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.15",
    "@babel/node": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^10.1.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "semver": ">=7.5.2"
  },
  "overrides": {
    "coveralls": {
      "request": {
        "tough-cookie": "^4.1.3",
        "qs": "^6.5.3"
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "pdc/**/*.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "docs/reports/coverage"
  },
  "scripts": {
    "start": "nodemon --max-old-space-size=2048 ./server.js --exec babel-node",
    "test": "nyc mocha --timeout=90000 --compilers js:@babel/register --require @babel/polyfill --no-deprecation",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "v": "node -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PDC/graphql.git"
  },
  "homepage": "https://github.com/PDC/graphql.git#readme"
}
